{
  "name": "gatsby-starter-gcn",
  "description": "A starter template to build amazing static websites with Gatsby, Contentful and Netlify",
  "version": "2.0.3",
  "repository": "https://github.com/ryanwiemer/gatsby-starter-gcn",
  "author": "Ryan Wiemer <ryan@ryanwiemer.com>",
  "dependencies": {
    "babel-plugin-styled-components": "^1.9.1",
    "chalk": "^2.4.1",
    "contentful-import": "^7.4.3",
    "gatsby": "^2.0.56",
    "gatsby-image": "^2.0.21",
    "gatsby-plugin-canonical-urls": "^2.0.7",
    "gatsby-plugin-catch-links": "^2.0.8",
    "gatsby-plugin-feed": "^2.0.9",
    "gatsby-plugin-google-analytics": "^2.0.7",
    "gatsby-plugin-manifest": "^2.0.10",
    "gatsby-plugin-netlify": "^2.0.5",
    "gatsby-plugin-nprogress": "^2.0.6",
    "gatsby-plugin-offline": "^2.0.17",
    "gatsby-plugin-react-helmet": "^3.0.2",
    "gatsby-plugin-sass": "^2.0.5",
    "gatsby-plugin-sitemap": "^2.0.2",
    "gatsby-plugin-styled-components": "^3.0.3",
    "gatsby-remark-autolink-headers": "^2.0.11",
    "gatsby-remark-images-contentful": "^2.0.4",
    "gatsby-remark-prismjs": "^3.1.0",
    "gatsby-source-contentful": "^2.0.15",
    "gatsby-source-filesystem": "^2.0.9",
    "gatsby-transformer-remark": "^2.1.13",
    "include-media": "^1.4.9",
    "inquirer": "^6.2.0",
    "lodash": "^4.17.11",
    "node-sass": "^4.10.0",
    "path": "^0.12.7",
    "prismjs": "^1.15.0",
    "react": "^16.6.3",
    "react-dom": "^16.6.3",
    "react-helmet": "^5.2.0",
    "snyk": "^1.112.0",
    "styled-components": "^4.1.1"
  },
  "keywords": [
    "gatsby"
  ],
  "license": "MIT",
  "main": "n/a",
  "scripts": {
    "build": "gatsby build",
    "develop": "gatsby develop",
    "test": "npm run lint:css && npm run lint:js && npm run check-pretty",
    "setup": "node ./bin/setup.js",
    "dev": "gatsby develop",
    "lint:js": "eslint --ext .js,.jsx .",
    "lint:css": "stylelint 'src/**/*.js'",
    "check-pretty": "prettier -l {,src/**/}*.{js,jsx,json,css,scss}",
    "format": "prettier --write \"src/**/*.js\"",
    "clean": "rm -rf public .cache",
    "snyk-protect": "snyk protect",
    "prepare": "npm run snyk-protect"
  },
  "devDependencies": {
    "eslint": "^5.9.0",
    "eslint-config-gatsby-standard": "^2.1.0",
    "prettier": "^1.15.2",
    "stylelint": "^13.0.0",
    "stylelint-config-prettier": "^4.0.0",
    "stylelint-config-standard": "^18.2.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-processor-styled-components": "^1.5.1"
  },
  "snyk": true
}
